var padding=func(len) {
    var res="";
    for(var i=0;i<len;i+=1) {
        res~="  ";
    }
    return res;
}

var bf=func(program) {
    var stack=[];
    var len=size(program);
    var f="#automatically generated by bfconvertor.nas\nvar ptr=0;\nvar paper=[];\nsetsize(paper,131072);\n";
    for(var i=0;i<len;i+=1) {
        var c=chr(program[i]);
        if(c=='+' or c=='-') {
            var cnt=0;
            for(;i<len;i+=1) {
                if(chr(program[i])=='+') {
                    cnt+=1;
                } elsif(chr(program[i])=='-') {
                    cnt-=1;
                } elsif(chr(program[i])!='\n') {
                    i-=1;
                    break;
                }
            }
            if(cnt!=0) {
                f~=padding(size(stack));
            }
            if(cnt>0) {
                f~="paper[ptr]+="~cnt~";\n";
            } elsif(cnt<0) {
                f~="paper[ptr]-="~(-cnt)~";\n";
            }
        } elsif(c=='<' or c=='>') {
            var cnt=0;
            for(;i<len;i+=1) {
                if(chr(program[i])=='>') {
                    cnt+=1;
                } elsif(chr(program[i])=='<') {
                    cnt-=1;
                } elsif(chr(program[i])!='\n') {
                    i-=1;
                    break;
                }
            }
            if(cnt!=0) {
                f~=padding(size(stack));
            }
            if(cnt>0) {
                f~="ptr+="~cnt~";\n";
            } elsif(cnt<0) {
                f~="ptr-="~(-cnt)~";\n";
            }
        } elsif(c==',') {
            f~=padding(size(stack));
            f~="paper[ptr]=input()[0];\n";
        } elsif(c=='.') {
            f~=padding(size(stack));
            f~="print(chr(paper[ptr]));\n";
        } elsif(c=='[') {
            f~=padding(size(stack));
            f~="while(paper[ptr]) {\n";
            append(stack,0);
        } elsif(c==']') {
            if(!size(stack)) {
                println("lack [");
                return;
            }
            pop(stack);
            f~=padding(size(stack));
            f~="}\n";
        }
    }
    if(size(stack)) {
        println("lack ]");
        return;
    }
    io.fout("mandel.nas",f);
    return;
}

bf(mandelbrot);